				<jointpos joint='j2s7s300_joint_fingertip_3'/>

				<!-- Distance data -->
				<rangefinder name="palm" site='palm'/>
				<rangefinder name="palm_1" site='palm_1'/>
				<rangefinder name="palm_2" site='palm_2'/>
